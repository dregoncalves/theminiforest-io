  "rich-text#question-1-1": {
    "props": {
      "text": "#### Onde fica a The Mini Forest?",
      "blockClass": "faq-question"
    }
  },
  "rich-text#answer-1-1": {
    "props": {
      "text": "A The Mini Forest é uma loja virtual que não realiza atendimento físico. O nosso escritório está localizado na Rua Guaçuí, 102 - Água Rasa - São Paulo - SP - 03178-180 e é destinado para o atendimento virtual e a recepção dos pedidos devolvidos.",
      "blockClass": "faq-answer"
    }
  },

