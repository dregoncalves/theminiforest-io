{
  "store.custom#sitemap": {
    "blocks": [
      "responsive-layout.desktop#sitemap",
      "responsive-layout.mobile#sitemap"
    ]
  },
  "responsive-layout.desktop#sitemap": {
    "children": ["flex-layout.row#sitemap-content-desktop"]
  },
  "responsive-layout.mobile#sitemap": {
    "children": ["flex-layout.row#sitemap-content-mobile"]
  },

  "flex-layout.row#sitemap-content-desktop": {
    "props": {
      "fullWidth": true,
      "horizontalAlign": "center",
      "paddingTop": 6,
      "paddingBottom": 6
    },
    "children": ["flex-layout.col#sitemap-wrapper"]
  },
  "flex-layout.col#sitemap-wrapper": {
    "props": {
      "width": "80%",
      "horizontalAlign": "center"
    },
    "children": ["rich-text#sitemap-title", "flex-layout.row#sitemap-list"]
  },
  "rich-text#sitemap-title": {
    "props": {
      "text": "# Mapa do site",
      "blockClass": "sitemapTitle"
    }
  },
  "flex-layout.row#sitemap-list": {
    "props": {
      "colGap": 5,
      "rowGap": 5,
      "preserveLayoutOnMobile": true,
      "blockClass": "mapa-do-site"
    },
    "children": [
      "flex-layout.col#roupinhas",
      "flex-layout.col#acessorios",
      "flex-layout.col#para-mamaes",
      "flex-layout.col#para-presente",
      "flex-layout.col#casa-e-decor",
      "flex-layout.col#higiene",
      "flex-layout.col#novidades",
      "flex-layout.col#sale",
      "flex-layout.col#estampas",
      "flex-layout.col#informacoes",
      "flex-layout.col#institucional"
    ]
  },

